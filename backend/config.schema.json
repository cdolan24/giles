{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "libraries": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "baseUrl": { "type": "string" },
          "apiKey": { "type": "string" },
          "enabled": { "type": "boolean" }
        },
        "required": ["name", "baseUrl", "enabled"]
      }
    }
  },
  "required": ["libraries"]
}